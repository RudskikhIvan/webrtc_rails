{
  "name": "WebRTC Screen Sharing",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "desktop.png",
    "128": "desktop.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["script.js"],
    "run_at": "document_start"
  }],
  "permissions": [
    "desktopCapture", "tabs"
  ]
}
