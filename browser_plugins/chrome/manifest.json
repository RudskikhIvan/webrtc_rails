{
  "name": "WebRTC Desktop",
  "version": "2",
  "manifest_version": 2,
  "icons": {
    "16": "desktop.png",
    "128": "desktop.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["http://*/*"],
    "js": ["script.js"]
  }],
  "browser_action": {
    "default_title": "Capture Screen"
  },
  "permissions": [
    "desktopCapture", "tabs"
  ]
}
