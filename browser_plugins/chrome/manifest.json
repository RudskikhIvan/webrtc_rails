{
  "name": "WebRTC Screen Sharing",
  "version": "1.3",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["script.js"],
    "run_at": "document_start"
  }],
  "permissions": [
    "desktopCapture",
    "tabs",
    "activeTab",
    "<all_urls>"
  ]
}
